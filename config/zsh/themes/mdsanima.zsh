    typeset -g POWERLEVEL9K_BACKGROUND_JOBS_VISUAL_IDENTIFIER_EXPANSION='üîß'

    #################[ virtualenv: python virtual environment ]#################
    typeset -g POWERLEVEL9K_VIRTUALENV_FOREGROUND=35
